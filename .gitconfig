[user]
  name = kimikimi714
  email = kimikimi714@gmail.com
[core]
  editor = vim
	excludesfile = /home/kimikimi714/go/src/github.com/kimikimi714/config/gitignoreGlobal
[color]
  ui = true
[alias]
	co = checkout
	st = status -sb # シンプルなstatus
	lg = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	gn = grep -n # grepに行番号をつける
	d  = diff --ignore-space-change
	b  = branch
	cv = commit -v
	ap = add -p
	pp = pull -p
[ghq]
	root = /home/kimikimi714/go/src
[commit]
	template = /home/kimikimi714/go/src/github.com/kimikimi714/config/emoji.md
